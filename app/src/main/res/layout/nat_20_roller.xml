<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.nat_20_expanded.Class"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/d12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="D12"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/d4"
        app:layout_constraintBaseline_toBaselineOf="@+id/d20"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/d4" />

    <Button
        android:id="@+id/d6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="D6"
        app:layout_constraintRight_toLeftOf="@+id/d4"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="23dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/d4"
        tools:layout_constraintBaseline_creator="1"
        android:layout_marginRight="23dp" />

    <Button
        android:id="@+id/d4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="D4"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="59dp"
        app:layout_constraintRight_toRightOf="@+id/d8"
        android:layout_marginTop="89dp"
        app:layout_constraintTop_toBottomOf="@+id/d12"
        android:layout_marginRight="59dp" />

    <Button
        android:id="@+id/d10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="D10"
        app:layout_constraintRight_toLeftOf="@+id/editText2"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText2"
        tools:layout_constraintBaseline_creator="1"
        android:layout_marginRight="16dp" />

    <Button
        android:id="@+id/d8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="D8"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="29dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText2"
        tools:layout_constraintBaseline_creator="1"
        android:layout_marginRight="29dp" />

    <Button
        android:id="@+id/d20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="D20"
        app:layout_constraintRight_toLeftOf="@+id/d12"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/d10"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="23dp"
        android:layout_marginRight="15dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="114dp"
        android:layout_height="42dp"
        android:ems="10"
        android:inputType="number"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/d20"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/d20" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="47dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="number"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/d12"
        app:layout_constraintRight_toRightOf="@+id/d20"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="24dp"
        app:layout_constraintLeft_toRightOf="@+id/d20" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Modifier"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/editText3"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="3dp"
        app:layout_constraintLeft_toRightOf="@+id/editText3"
        android:layout_marginLeft="18dp" />

</android.support.constraint.ConstraintLayout>
